        <!-- Content -->
        <main>
            <!-- Features -->
            <section>
                <div class="d-flex justify-content-center align-items-center mt-5" data-aos="fade-up"
                    data-aos-duration="500" data-aos-delay="50" data-aos-easing="ease-in-out">
                    <h1 class="display-1 ff-primary">We Offer</h1>
                </div>
                <div class="row mx-sm-3 mx-auto py-5 my-3 def-border" data-aos="fade-up" data-aos-duration="1000"
